{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "eslint:recommended",
        "airbnb"
    ],
    "parser": "babel-eslint",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/sort-comp": 0,
        "semi": 0,
        "react/destructuring-assignment": 0,
        "react/prop-types": 0,
        "react/jsx-filename-extension": 0,
        "comma-dangle": 0,
        "prefer-destructuring": 0,
        "react/jsx-props-no-spreading": 0,
        "react/prefer-stateless-function": 0,
        "no-param-reassign": 0,
        "object-curly-newline": 0,
        "class-methods-use-this": 0
    }
}
